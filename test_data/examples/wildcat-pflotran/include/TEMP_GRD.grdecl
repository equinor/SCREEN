
DIMENS 
20 20 80 /

--Get LGR. File is generated by FMU
external_file ../include/TEMP_LGR.grdecl / 

--LWRES_LGR_T11.grdecl  / 
 

--Get Tops and DZ definitions. File is generated by FMU

external_file ../include/tops_dz.inc

--EQUALS 
--TOPS  4  4* 1 1 /
--/
--
--DZ 
--400*40 400*110 3200*150  20000*5 /



EQUALS
FIPLEG 4  / 
DX    200   /
DY    200  /
PORO   0.2   /
PERMX  1000 / 


----------------------------------------------------
-- EQUILIBRATION REGIONS inside the main grid
----------------------------------------------------
EQLNUM 1 1 20 1 20 1 10 / 
EQLNUM 2 1 20 1 20 11 80 / 


--300.0 /
-- models overburden
PERMX  0.001 4* 1 10 /
FIPLEG 2 4* 1 10 / 
--models sea bed conditions 
PERMX  10000 4* 1 1 / 
PORO  1.0    4* 1 1 / 
FIPLEG  1    4* 1 1 / 
--models the Reservoir conditions 
PERMX  0.01 4* 11 11 /

--Set FIPNUM (FIPLEG) for reservoir
FIPLEG  3 4* 11 11 /

--aquifer FIPLEG
FIPLEG 5 4* 78 80 /


TRANZ  0 4* 11 11 / 
TRANX  0 4* 11 11 / 
TRANY  0 4* 11 11 / 
/


COPY 
PERMX PERMY /
PERMX PERMZ /
/

MULTIPLY
--Aquifer pore volume increase
PORV 2000.0 4* 78 80 /
--Vertical permeability reduction
PERMZ 0.1 1 20 1 20 1 80 / 
/

MINPV
 0 /

